{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": true,
        "arrow-parens": false,
        "prefer-const": true,
        "variable-name": false,
        "triple-equals": false,
        "max-line-length": false,
        "only-arrow-functions": false,
        "class-name": false,
        "align": false,
        "no-bitwise": false,
        "no-unused-expression": true,
        "no-console": false,
        "no-shadowed-variable": false,
        "prefer-for-of": true,
        "max-classes-per-file": false,
        "no-construct": true,
        "strict-boolean-expressions": true
    },
    "rulesDirectory": []
}