{
  "scripts": {
    "bootstrap": "node -r dotenv/config bootstrap-entry.js",
    "deploy-local": "DEBUG=obs,obs:* node -r dotenv/config ./node_modules/serverless/bin/serverless.js deploy --config serverless-local.js",
    "deploy-local-mac": "npm run linux-container -- npm run package && npm run deploy-local -- --package .serverless-package",
    "install-mac": "npm run linux-container -- npm i",
    "linux-container": "docker run --rm -v \"$PWD\":/var/task lambci/lambda:build-nodejs12.x ",
    "lint": "npx eslint .",
    "package": "DEBUG=obs,obs:* node -r dotenv/config ./node_modules/serverless/bin/serverless.js package --stage local --service obs --package .serverless-package",
    "start-localstack": "SERVICES=serverless DEBUG=true DEFAULT_REGION=us-east-1 LAMBDA_EXECUTOR=docker-reuse localstack start --docker",
    "start-localstack-mac": "TMPDIR=/private$TMPDIR npm run start-localstack",
    "typecheck": "tsc -p . --noEmit",
    "test": "npm run typecheck && npm run test:unit && npm run test:integration",
    "test:integration": "mocha --require dotenv/config tests/integration --recursive --timeout 60000",
    "test:unit": "mocha --require dotenv/config --require esm tests/unit --recursive --timeout 60000",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
